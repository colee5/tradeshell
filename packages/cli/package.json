{
	"name": "@tradeshell/cli",
	"version": "0.0.0",
	"license": "MIT",
	"bin": {
		"tradeshell": "dist/cli.js"
	},
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "tsc",
		"dev": "./dev-watch.sh",
		"dev:once": "DEV=true bun source/cli.tsx",
		"start": "bun source/cli.tsx",
		"shell": "bun source/cli.tsx",
		"check-types": "tsc --noEmit",
		"test": "prettier --check . && xo && ava",
		"prepare": "husky"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@hookform/resolvers": "^5.2.2",
		"@tanstack/react-query": "^5.90.16",
		"@tradeshell/core": "workspace:*",
		"@types/node": "^25.2.0",
		"figures": "^6.1.0",
		"ink": "^6.6.0",
		"ink-big-text": "^2.0.0",
		"ink-select-input": "^6.2.0",
		"ink-spinner": "^5.0.0",
		"ink-syntax-highlight": "^2.0.2",
		"ink-text-input": "^6.0.0",
		"jotai": "^2.16.1",
		"react": "^19.2.3",
		"react-hook-form": "^7.70.0",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/react": "^18.0.32",
		"ava": "^5.2.0",
		"chalk": "^5.2.0",
		"ink-testing-library": "^3.0.0",
		"typescript": "^5.0.3"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	}
}
