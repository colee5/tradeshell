{
	"name": "@tradeshell/cli",
	"version": "0.0.0",
	"license": "MIT",
	"bin": {
		"tradeshell": "dist/cli.js"
	},
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "tsc",
		"dev": "./dev-watch.sh",
		"dev:once": "DEV=true tsx source/cli.tsx",
		"start": "tsx source/cli.tsx",
		"shell": "tsx source/cli.tsx",
		"check-types": "tsc --noEmit",
		"test": "prettier --check . && xo && ava",
		"generate:api": "openapi-ts",
		"prepare": "husky"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@hey-api/client-axios": "^0.9.1",
		"@tanstack/react-query": "^5.90.16",
		"axios": "^1.13.2",
		"ink": "^6.6.0",
		"ink-big-text": "^2.0.0",
		"ink-select-input": "^6.2.0",
		"ink-spinner": "^5.0.0",
		"ink-syntax-highlight": "^2.0.2",
		"ink-text-input": "^6.0.0",
		"jotai": "^2.16.1",
		"react": "^19.2.3",
		"react-hook-form": "^7.70.0",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@commitlint/cli": "^20.3.1",
		"@commitlint/config-conventional": "^20.3.1",
		"@hey-api/openapi-ts": "^0.90.3",
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/react": "^18.0.32",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"chalk": "^5.2.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"husky": "^9.1.7",
		"ink-testing-library": "^3.0.0",
		"lint-staged": "^16.2.7",
		"prettier": "^2.8.7",
		"ts-node": "^10.9.1",
		"tsx": "^4.7.0",
		"typescript": "^5.0.3",
		"xo": "^0.54.2"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"lint-staged": {
		"*.{ts,tsx}": [
			"xo",
			"prettier --write"
		]
	}
}
