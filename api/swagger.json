    "/config/blockchain": {
      "put": {
        "operationId": "ConfigController_updateBlockchainConfig",
        "parameters": [],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/BlockchainConfigDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ConfigDto"
                }
              }
            }
          }
        },
        "summary": "Update blockchain configuration",
        "tags": [
          "config"
        ]
      }
    },
    "/config/chains": {
      "get": {
        "operationId": "ConfigController_getChains",
        "parameters": [],
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ChainsResponseDto"
                }
              }
            }
          }
        },
        "summary": "Get supported blockchain chains",
        "tags": [
          "config"
        ]
      }
    },
    "/wallet/store": {
      "post": {
        "operationId": "WalletController_storePrivateKey",
        "parameters": [],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/StorePrivateKeyDto"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Private key stored successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/WalletResponseDto"
                }
              }
            }
          },
          "400": {
            "description": "Invalid private key or password"
          }
        },
        "summary": "Store a private key encrypted with a password",
        "tags": [
          "wallet"
        ]
      }
    },
    "/wallet/unlock": {
      "post": {
        "operationId": "WalletController_unlockWallet",
        "parameters": [],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UnlockWalletDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Wallet unlocked successfully"
          },
          "401": {
            "description": "Invalid password"
          },
          "404": {
            "description": "Wallet not found"
          }
        },
        "summary": "Unlock a wallet for the current session",
        "tags": [
          "wallet"
        ]
      }
    },
    "/wallet/lock": {
      "post": {
        "operationId": "WalletController_lockWallets",
        "parameters": [],
        "responses": {
          "200": {
            "description": "All wallets locked successfully"
          }
        },
        "summary": "Lock all wallets",
        "tags": [
          "wallet"
        ]
      }
    },
    "/wallet/active/{address}": {
      "post": {
        "operationId": "WalletController_setActiveWallet",
        "parameters": [
          {
            "name": "address",
            "required": true,
            "in": "path",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Active wallet set successfully"
          },
          "404": {
            "description": "Wallet not unlocked"
          }
        },
        "summary": "Set active wallet (must be unlocked first)",
        "tags": [
          "wallet"
        ]
      }
    },
    "/wallet/status": {
      "get": {
        "operationId": "WalletController_getStatus",
        "parameters": [],
        "responses": {
          "200": {
            "description": "Wallet status retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/WalletStatusDto"
                }
              }
            }
          }
        },
        "summary": "Get the current wallet status",
        "tags": [
          "wallet"
        ]
      }
    },
    "/wallet/list": {
      "get": {
        "operationId": "WalletController_listWallets",
        "parameters": [],
        "responses": {
          "200": {
            "description": "Wallets listed successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/WalletListDto"
                }
              }
            }
          }
        },
        "summary": "List all wallets in the keystore",
        "tags": [
          "wallet"
        ]
      }
    },
    "/wallet/password": {
      "put": {
        "operationId": "WalletController_changePassword",
        "parameters": [],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ChangePasswordDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Password changed successfully"
          },
          "401": {
            "description": "Invalid old password"
          },
          "404": {
            "description": "Wallet not found"
          }
        },
        "summary": "Change the password for a wallet",
        "tags": [
          "wallet"
        ]
      }
    },
    "/wallet/{address}": {
      "delete": {
        "operationId": "WalletController_deleteWallet",
        "parameters": [
          {
            "name": "address",
            "required": true,
            "in": "path",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Wallet deleted successfully"
          },
          "404": {
            "description": "Wallet not found"
          }
        },
        "summary": "Delete a wallet from the keystore",
        "tags": [
          "wallet"
        ]
      }
    }
  },
  "info": {
    "title": "TradeShell API",
    "description": "API for TradeShell CLI",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "schemas": {
      "LlmConfigDto": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "enum": [
              "cloud",
              "self-hosted"
            ],
            "description": "LLM provider type"
          },
          "provider": {
            "type": "string",
            "description": "Cloud provider name (anthropic, openai, groq)"
          },
          "model": {
            "type": "string",
            "description": "Model name"
          },
          "baseURL": {
            "type": "string",
            "description": "Base URL for self-hosted LLM"
          },
          "apiKey": {
            "type": "string",
            "description": "API key for authentication"
          }
        }
      },
      "BlockchainConfigDto": {
        "type": "object",
        "properties": {
          "chainId": {
            "enum": [
              1,
              8453,
              42161,
              11155111
            ],
            "type": "number",
            "description": "Chain ID",
            "example": 8453
          },
          "rpcUrl": {
            "type": "string",
            "format": "uri",
            "description": "RPC URL"
          }
        },
        "required": [
          "chainId"
        ]
      },
      "ConfigDto": {
        "type": "object",
        "properties": {
          "llm": {
            "description": "LLM configuration",
            "allOf": [
              {
                "$ref": "#/components/schemas/LlmConfigDto"
              }
            ]
          },
          "blockchain": {
            "description": "Blockchain configuration",
            "allOf": [
              {
                "$ref": "#/components/schemas/BlockchainConfigDto"
              }
            ]
          }
        }
      },
      "ChainNativeCurrencyDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Ether"
          },
          "symbol": {
            "type": "string",
            "example": "ETH"
          },
          "decimals": {
            "type": "number",
            "example": 18
          }
        },
        "required": [
          "name",
          "symbol",
          "decimals"
        ]
      },
      "ChainDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number",
            "example": 8453,
            "description": "Chain ID in number form"
          },
          "name": {
            "type": "string",
            "example": "Base",
            "description": "Human-readable chain name"
          },
          "nativeCurrency": {
            "$ref": "#/components/schemas/ChainNativeCurrencyDto"
          },
          "rpcUrls": {
            "type": "object",
            "description": "Collection of RPC endpoints"
          },
          "blockExplorers": {
            "type": "object",
            "description": "Collection of block explorers"
          },
          "testnet": {
            "type": "boolean",
            "example": false
          },
          "sourceId": {
            "type": "number",
            "example": 1,
            "description": "Source Chain ID (L1 for L2s)"
          }
        },
        "required": [
          "id",
          "name",
          "nativeCurrency",
          "rpcUrls"
        ]
      },
      "ChainsResponseDto": {
        "type": "object",
        "properties": {
          "chains": {
            "description": "List of supported blockchain chains",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ChainDto"
            }
          }
        },
        "required": [
          "chains"
        ]
      },
      "StorePrivateKeyDto": {
        "type": "object",
        "properties": {
          "privateKey": {
            "type": "string",
            "description": "Private key to store (with or without 0x prefix)",
            "example": "0x1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef"
          },
          "password": {
            "type": "string",
            "minLength": 8,
            "description": "Password to encrypt the private key",
            "example": "my-secure-password"
          },
          "setActive": {
            "type": "boolean",
            "description": "Whether to unlock and set as active wallet (default: true)",
            "example": true,
            "default": true
          }
        },
        "required": [
          "privateKey",
          "password"
        ]
      },
      "WalletResponseDto": {
        "type": "object",
        "properties": {
          "address": {
            "type": "string",
            "description": "Wallet address",
            "example": "0x742d35Cc6634C0532925a3b844Bc9e7595f0bEb0"
          }
        },
        "required": [
          "address"
        ]
      },
      "UnlockWalletDto": {
        "type": "object",
        "properties": {
          "address": {
            "type": "string",
            "description": "Wallet address",
            "example": "0x742d35Cc6634C0532925a3b844Bc9e7595f0bEb0"
          },
          "password": {
            "type": "string",
            "description": "Password to decrypt the private key",
            "example": "my-secure-password"
          }
        },
        "required": [
          "address",
          "password"
        ]
      },
      "WalletStatusDto": {
        "type": "object",
        "properties": {
          "unlocked": {
            "type": "boolean",
            "description": "Whether any wallet is currently unlocked",
            "example": true
          },
          "activeAddress": {
            "type": "string",
            "description": "Address of the currently active wallet",
            "example": "0x742d35Cc6634C0532925a3b844Bc9e7595f0bEb0"
          },
          "unlockedWallets": {
            "description": "List of all unlocked wallet addresses",
            "example": [
              "0x742d35Cc6634C0532925a3b844Bc9e7595f0bEb0",
              "0x1234567890abcdef1234567890abcdef12345678"
            ],
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "required": [
          "unlocked",
          "unlockedWallets"
        ]
      },
      "WalletListDto": {
        "type": "object",
        "properties": {
          "wallets": {
            "description": "List of wallet addresses in the keystore",
            "example": [
              "0x742d35Cc6634C0532925a3b844Bc9e7595f0bEb0",
              "0x1234567890abcdef1234567890abcdef12345678"
            ],
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "required": [
          "wallets"
        ]
      },
      "ChangePasswordDto": {
        "type": "object",
        "properties": {
          "address": {
            "type": "string",
            "description": "Wallet address",
            "example": "0x742d35Cc6634C0532925a3b844Bc9e7595f0bEb0"
          },
          "oldPassword": {
            "type": "string",
            "description": "Current password",
            "example": "my-old-password"
          },
          "newPassword": {
            "type": "string",
            "minLength": 8,
            "description": "New password (at least 8 characters)",
            "example": "my-new-password"
          }
        },
        "required": [
          "address",
          "oldPassword",
          "newPassword"
        ]
      }
    }
  }
}