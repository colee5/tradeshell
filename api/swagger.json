  "info": {
    "title": "TradeShell API",
    "description": "API for TradeShell CLI",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "schemas": {
      "LlmConfigDto": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "enum": [
              "cloud",
              "self-hosted"
            ],
            "description": "LLM provider type"
          },
          "provider": {
            "type": "string",
            "description": "Cloud provider name (anthropic, openai, groq)"
          },
          "model": {
            "type": "string",
            "description": "Model name"
          },
          "baseURL": {
            "type": "string",
            "description": "Base URL for self-hosted LLM"
          },
          "apiKey": {
            "type": "string",
            "description": "API key for authentication"
          }
        }
      },
      "BlockchainConfigDto": {
        "type": "object",
        "properties": {
          "chainId": {
            "enum": [
              1,
              8453,
              42161,
              11155111
            ],
            "type": "number",
            "description": "Chain ID",
            "example": 8453
          },
          "rpcUrl": {
            "type": "string",
            "format": "uri",
            "description": "RPC URL"
          }
        },
        "required": [
          "chainId"
        ]
      },
      "ConfigDto": {
        "type": "object",
        "properties": {
          "llm": {
            "description": "LLM configuration",
            "allOf": [
              {
                "$ref": "#/components/schemas/LlmConfigDto"
              }
            ]
          },
          "blockchain": {
            "description": "Blockchain configuration",
            "allOf": [
              {
                "$ref": "#/components/schemas/BlockchainConfigDto"
              }
            ]
          }
        }
      },
      "ChainNativeCurrencyDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Ether"
          },
          "symbol": {
            "type": "string",
            "example": "ETH"
          },
          "decimals": {
            "type": "number",
            "example": 18
          }
        },
        "required": [
          "name",
          "symbol",
          "decimals"
        ]
      },
      "ChainDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number",
            "example": 8453,
            "description": "Chain ID in number form"
          },
          "name": {
            "type": "string",
            "example": "Base",
            "description": "Human-readable chain name"
          },
          "nativeCurrency": {
            "$ref": "#/components/schemas/ChainNativeCurrencyDto"
          },
          "rpcUrls": {
            "type": "object",
            "description": "Collection of RPC endpoints"
          },
          "blockExplorers": {
            "type": "object",
            "description": "Collection of block explorers"
          },
          "testnet": {
            "type": "boolean",
            "example": false
          },
          "sourceId": {
            "type": "number",
            "example": 1,
            "description": "Source Chain ID (L1 for L2s)"
          }
        },
        "required": [
          "id",
          "name",
          "nativeCurrency",
          "rpcUrls"
        ]
      },
      "ChainsResponseDto": {
        "type": "object",
        "properties": {
          "chains": {
            "description": "List of supported blockchain chains",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ChainDto"
            }
          }
        },
        "required": [
          "chains"
        ]
      },
      "SetupPasswordBodyDto": {
        "type": "object",
        "properties": {
          "password": {
            "type": "string",
            "minLength": 4,
            "example": "mypass"
          }
        },
        "required": [
          "password"
        ]
      },
      "UnlockBodyDto": {
        "type": "object",
        "properties": {
          "password": {
            "type": "string",
            "example": "mypass"
          }
        },
        "required": [
          "password"
        ]
      },
      "ChangePasswordBodyDto": {
        "type": "object",
        "properties": {
          "oldPassword": {
            "type": "string",
            "example": "oldpass"
          },
          "newPassword": {
            "type": "string",
            "minLength": 4,
            "example": "newpass"
          }
        },
        "required": [
          "oldPassword",
          "newPassword"
        ]
      },
      "AddWalletBodyDto": {
        "type": "object",
        "properties": {
          "privateKey": {
            "type": "string",
            "pattern": "hashRegex",
            "example": "0x1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef"
          },
          "name": {
            "type": "string",
            "example": "Main Trading"
          },
          "setActive": {
            "type": "boolean",
            "example": true,
            "default": true
          }
        },
        "required": [
          "privateKey",
          "name"
        ]
      },
      "WalletResponseDto": {
        "type": "object",
        "properties": {
          "address": {
            "type": "string",
            "example": "0x742d35Cc6634C0532925a3b844Bc9e7595f0bEb0"
          }
        },
        "required": [
          "address"
        ]
      },
      "WalletInfoResponseDto": {
        "type": "object",
        "properties": {
          "address": {
            "type": "string",
            "example": "0x742d35Cc6634C0532925a3b844Bc9e7595f0bEb0"
          },
          "name": {
            "type": "string",
            "example": "Main Trading"
          },
          "isActive": {
            "type": "boolean",
            "example": true
          }
        },
        "required": [
          "address",
          "name",
          "isActive"
        ]
      },
      "WalletListResponseDto": {
        "type": "object",
        "properties": {
          "wallets": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/WalletInfoResponseDto"
            }
          }
        },
        "required": [
          "wallets"
        ]
      },
      "WalletStatusResponseDto": {
        "type": "object",
        "properties": {
          "isSetup": {
            "type": "boolean",
            "example": true
          },
          "isUnlocked": {
            "type": "boolean",
            "example": true
          },
          "activeAddress": {
            "type": "string",
            "nullable": true,
            "example": "0x742d35Cc6634C0532925a3b844Bc9e7595f0bEb0"
          },
          "activeName": {
            "type": "string",
            "nullable": true,
            "example": "Main Trading"
          },
          "walletCount": {
            "type": "number",
            "example": 2
          }
        },
        "required": [
          "isSetup",
          "isUnlocked",
          "walletCount"
        ]
      },
      "SetActiveWalletDto": {
        "type": "object",
        "properties": {
          "address": {
            "type": "string",
            "example": "0x742d35Cc6634C0532925a3b844Bc9e7595f0bEb0"
          }
        },
        "required": [
          "address"
        ]
      },
      "DeleteWalletDto": {
        "type": "object",
        "properties": {
          "address": {
            "type": "string",
            "example": "0x742d35Cc6634C0532925a3b844Bc9e7595f0bEb0"
          }
        },
        "required": [
          "address"
        ]
      }
    }
  }
}